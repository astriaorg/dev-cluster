{
  "app_hash": "",
  "app_state": {
    "accounts": {{ toPrettyJson .Values.config.sequencer.genesisAccounts | indent 4 | trim }},
    "authority_sudo_key": "{{ .Values.config.sequencer.sudoAuthorityKey }}"
  },
  "chain_id": "{{ .Values.config.cometBFT.chainId }}",
  "consensus_params": {
    "block": {
      "max_bytes": "22020096",
      "max_gas": "-1"
    },
    "evidence": {
      "max_age_duration": "172800000000000",
      "max_age_num_blocks": "100000",
      "max_bytes": "1048576"
    },
    "validator": {
      "pub_key_types": [
        "ed25519"
      ]
    },
    "version": {
      "app": "0"
    }
  },
  "genesis_time": "2023-09-22T17:22:35.092832Z",
  "initial_height": "0",
  "validators": [
  {{- range $index, $value := .Values.config.cometBFT.validators }}
      {{- if $index }},{{- end }}
      {
        "address": "{{ $value.address }}",
        "name": "{{ $value.name }}",
        "power": "{{ $value.power }}",
        "pub_key": {
          "type": "tendermint/PubKeyEd25519",
          "value": "{{ $value.pubKey }}"
        }
      }
  {{- end }}
  ]
}
