{
    "config": {
        "chainId": {{ .Values.config.rollup.networkId }},
        "homesteadBlock": 0,
        "eip150Block": 0,
        "eip155Block": 0,
        "eip158Block": 0,
        "byzantiumBlock": 0,
        "constantinopleBlock": 0,
        "petersburgBlock": 0,
        "istanbulBlock": 0,
        "berlinBlock": 0,
        "londonBlock": 0,
        "terminalTotalDifficulty": 0,
        "terminalTotalDifficultyPassed": true,
        "ethash": {},
        {{- if .Values.config.rollup.extraDataOverride }}
        "astriaExtraDataOverride": "{{ .Values.config.rollup.extraDataOverride }}",
        {{ end -}}
        "astriaOverrideGenesisExtraData": {{ .Values.config.rollup.overrideGenesisExtraData }},
        "astriaSequencerInitialHeight": {{ .Values.config.sequencer.initialBlockHeight }},
        "astriaDataAvailabilityInitialHeight": {{ .Values.config.celestia.initialBlockHeight }},
        "astriaDataAvailabilityHeightVariance": {{ .Values.config.celestia.heightVariance }}
    },
    "difficulty": "10000000",
    "gasLimit": "8000000",
    "alloc": {
    {{- range $index, $value := .Values.config.rollup.genesisAccounts }}
        {{- if $index }},{{- end }}
        "{{ $value.address }}": { "balance": "{{ $value.balance }}" }
    {{- end }}
    }
}
